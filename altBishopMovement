downright: {
                for (var i = (x+1); i < board.length; i++) {
                        for (var j = (y+i-x); (j-y)==(i-x); j++) {
                                if( isEmpty(i,j) ) {
                                       moves.push(i+ ',' +j);
                                } else {
                                       break downright;
                                }
                     	}
                }
        }
